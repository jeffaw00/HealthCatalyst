@model HealthCatalystPeopleSearch.Models.Persons

<div class="table-responsive">
    <table class="table">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>Zip</th>
                <th>Age</th>
                <th>Interests</th>
                <th>Image Location</th>
                <th></th>
            </tr>
        </thead>
        @if (Model.persons != null)
        {
            foreach (var item in Model.persons)
            {
                <tr>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                    <td>@item.Address</td>
                    <td>@item.City</td>
                    <td>@item.State</td>
                    <td>@item.Zip</td>
                    <td>@item.Age</td>
                    <td>@item.Interests</td>
                    <td>@item.PicLocation</td>
                    <td>
                        <button name="DeletePerson" type="button" class="btn btn-default" data-id="@item.PersonId">Delete</button>
                    </td>
                </tr>
            }
        }
        else
        {
            <tr><td>No Data</td></tr>
        }
    </table>
</div>

